{
  "name": "keeper_mern",
  "version": "1.0.0",
  "description": "Keeper built with the MERN stack",
  "main": "server.js",
  "engine": {
    "node": "14.16.0",
    "npm": "7.10.0"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Dishari Das",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.2",
    "express": "^4.17.1",
    "mongoose": "^5.12.4",
    "read-scripts": "^1.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
